{
  "name": "rougin/dextra",
  "description": "\"Ready-to-eat\" CRUD for frontend.",
  "keywords": [ "php-crud", "php-ui", "ui-template" ],
  "homepage": "https://github.com/rougin/dextra",
  "license": "MIT",
  "authors":
  [
    {
      "email": "rougingutib@gmail.com",
      "homepage": "https://roug.in/",
      "name": "Rougin Gutib",
      "role": "Software Engineer"
    }
  ],
  "require":
  {
    "php": ">=5.3.0",
    "tedivm/jshrink": "~1.0"
  },
  "require-dev":
  {
    "phpunit/phpunit": "*",
    "sanmai/phpunit-legacy-adapter": "*"
  },
  "autoload":
  {
    "psr-4":
    {
      "Rougin\\Dextra\\": "src"
    }
  },
  "autoload-dev":
  {
    "psr-4":
    {
      "Rougin\\Dextra\\": "tests"
    }
  },
  "scripts":
  {
    "analyze": "phpstan analyse",
    "restyle": "php-cs-fixer fix --config=phpstyle.php",
    "test": "phpunit"
  }
}
